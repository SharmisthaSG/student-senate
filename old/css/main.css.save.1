body {
  background-color: #f2f2f2;
  font-family: "Open Sans";
  font-weight: 300;
  font-size: 22px;
  padding-top: 50px;
}

h1, h2, h3, h4, h5, h6 {
  font-family: "Open Sans";
  font-weight: 300;
 
}

#mems
{
	transition: all .6 ease-out;
}
#mems col-md-3, #mems col-sm-6

a{
  color: #111111;
  font-size: 30px;
}

.centered {
  text-align: center;
}

#header {
  background:url("../img/cover.jpg");
  background-repeat: no-repeat;
  background-position: center center;
    
  padding-top: 30vh;
  padding-bottom: 25vh;
  height:100vh;
  position: relative;
}


#header h1 {
  margin-top: 10px;
  color: white;
  font-size: 70px;
  
}

#content {
  padding-top: 50px;
  /*padding-bottom: 50px;*/
  border-top: #808080 solid 1px;
}

#content h1{
  padding-bottom: 50px;
  font-size: 60px;
  color: #111111;
}

#content h2{
  font-size: 40px;
  color: #111111;
}

#content h3{
  font-size: 30px;
  color: #111111;
}

#content p{
  color: #222222;
}

#whatsnew {
  display: block;
  padding-bottom: 50px;
  background-color: #CECECE;
}

#whatsnew h1 {
  padding-bottom: 50px;
  color: #222222;
  font-size: 60px;
}

#contact {
  
  /*background:url("../img/cover2.png");
  background-repeat: no-repeat;
  background-position: center center;*/
  background-color: #333333;
  color: white;
  padding-top: 20px;
  padding-bottom: 40px;
  text-align: center;
}

#contact h1{
  font-size: 60px;
}

#contact a{
  color: #FFFFFF;
  font-size: 30px;
}

#nvb a{
  font-size: 20px;
}

.IIITDlogo {
  min-width: 300px;
  width: 80%;
  max-width: 500px;
  padding-bottom: 20px; 
}
